{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AppHub FastAPI (uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--host", "0.0.0.0", "--port", "8500"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/apphub-backend",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/apphub-backend"
      }
    }
  ]
}
